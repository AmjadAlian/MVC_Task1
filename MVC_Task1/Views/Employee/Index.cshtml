@model List<Employee>

    <h2>
        Employees
    </h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
           
            <th scope="col">Name</th>
            <th scope="col">Title</th>
            <th scope="col">Age</th>

        </tr>
    </thead>
    <tbody>
        
        @foreach (var item in Model)
        {
           <tr>
            <td scope="row">@item.Id</td>
            <td>@item.Name</td>
            <td>@item.title</td>
             <td>@item.Age</td>
        </tr> 
        }
        
    </tbody>
</table>